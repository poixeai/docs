{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Poixe Docs",
  "colors": {
    "primary": "#111111",
    "light": "#FFFFFF",
    "dark": "#111111"
  },
  "favicon": "/assets/logo/favicon.ico",
  "navigation": {
    "languages": [
      {
        "language": "en",
        "default": true,
        "tabs": [
          {
            "tab": "Docs",
            "groups": [
              { "group": "Getting started", "pages": ["en/quickstart"] }
            ]
          },
          {
            "tab": "API reference",
            "groups": [
              { "group": "Getting started", "pages": ["en/development"] }
            ]
          }
        ]
      },
      {
        "language": "cn",
        "tabs": [
          {
            "tab": "Docs",
            "groups": [
              { "group": "Getting started", "pages": ["cn/quickstart"] }
            ]
          },
          {
            "tab": "API reference",
            "groups": [
              { "group": "Getting started", "pages": ["cn/development"] }
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/assets/logo/poixe_docs_light.svg",
    "dark": "/assets/logo/poixe_docs_dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://poixe.com/about/contact"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://poixe.com/dashboard"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/PoixeAI",
      "github": "https://github.com/poixeai/"
    }
  }
}
